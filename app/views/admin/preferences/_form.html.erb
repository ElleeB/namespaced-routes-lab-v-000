<%= form_for @preference do |f| %>
  <h4><p>Artist Sort Order <%= f.text_field :artist_sort_order %> </p></h4>
  <h4><p>Song Sort Order <%= f.text_field :song_sort_order %> </p></h4>
  <h4><p>Create Artists Permission <%= f.text_field :allow_create_artists %> </p></h4>
  <h4><p>Create Songs Permission <%= f.text_field :allow_create_songs %> </p></h4>
  <p> <%= f.submit %> </p>
<% end %>
